    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint 'src/**/*.{ts,js}' --format json -o eslint-report.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --coverageReporters=lcov --coverageReporters=text-summary",
    "fix:coverage": "node -e \"const fs=require('fs');const f='coverage/lcov.info';if(fs.existsSync(f)){fs.writeFileSync(f,fs.readFileSync(f,'utf8').replace(/\\\\/g,'/'));console.log('Rutas normalizadas en lcov.info');}else{console.log('No se encontr√≥ coverage/lcov.info');}\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
